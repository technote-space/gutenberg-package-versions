	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"editor"
	],
	"engines": {
		"node": ">=8.0.0",
		"npm": ">=6.0.0"
	},
	"dependencies": {
		"@wordpress/a11y": "file:packages/a11y",
		"@wordpress/api-fetch": "file:packages/api-fetch",
		"@wordpress/autop": "file:packages/autop",
